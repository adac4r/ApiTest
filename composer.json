{
    "name": "ubuntu/api",
    "license": "proprietary",
    "type": "project",
    "autoload": {
        "psr-4": {
            "": "src/"
        },
        "classmap": [
            "app/AppKernel.php",
            "app/AppCache.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "require": {
        "php": ">=5.5.9",
        "symfony/symfony": "3.1.*",
        "doctrine/orm": "^2.5",
        "doctrine/dbal": "~2.5",
        "doctrine/doctrine-bundle": "~1.3",
        "twig/extensions": "~1.3.0",
        "symfony/assetic-bundle": "~2.3",
        "symfony/swiftmailer-bundle": "~2.3",
        "symfony/monolog-bundle": "~2.4",
        "sensio/distribution-bundle": "~4.0",
        "sensio/framework-extra-bundle": "~3.0,>=3.0.2",
        "incenteev/composer-parameter-handler": "^2.1",
        "jms/serializer-bundle": "^1.0",
        "zendframework/zend-code": "3.0.2",
        "friendsofsymfony/user-bundle": "~2.0@dev",
        "friendsofsymfony/rest-bundle": "^1.8@dev",
        "friendsofsymfony/oauth-server-bundle": "^1.4",
        "friendsofsymfony/jsrouting-bundle": "^1.5",
        "nelmio/api-doc-bundle": "^2.10.3",
        "willdurand/hateoas-bundle": "^1.0",
      
        "nelmio/security-bundle": "^1.8",
        "nelmio/cors-bundle": "dev-master",
        "fresh/doctrine-enum-bundle": "v4.0",
        "gregwar/image-bundle": "^2.1",
        "stof/doctrine-extensions-bundle": "^1.2",
        "flowjs/flow-php-server": "dev-master#16cc23f",
        "knplabs/gaufrette": "^0.2.1",
        "knplabs/knp-gaufrette-bundle": "^0.3.0",
        "aws/aws-sdk-php": "^3.18",
        "aws/aws-sdk-php-symfony": "^1.1",
        "doctrine/doctrine-migrations-bundle": "^1.0"
    },
    "require-dev": {
        "sensio/generator-bundle": "^3.0",
        "symfony/phpunit-bridge": "^3.0"
    },
    "scripts": {
        "post-install-cmd": [
            "Incenteev\\ParameterHandler\\ScriptHandler::buildParameters",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installAssets",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installRequirementsFile",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::prepareDeploymentTarget"
        ],
        "post-update-cmd": [
            "Incenteev\\ParameterHandler\\ScriptHandler::buildParameters",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installAssets",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installRequirementsFile",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::prepareDeploymentTarget"
        ]
    },
    "extra": {
        "symfony-app-dir": "app",
        "symfony-bin-dir": "bin",
        "symfony-var-dir": "var",
        "symfony-web-dir": "web",
        "symfony-tests-dir": "tests",
        "symfony-assets-install": "relative",
    "incenteev-parameters": 
      {
        "file": "app/config/parameters.yml",
       }
  }
}
